{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "trailing-comma": false,
        "eofline": false,
        "ordered-imports": false,
        "no-consecutive-blank-lines": false,
        "arrow-parens": {
            "options":"ban-single-arg-parens"
        },
        "no-console": false,
        "object-literal-sort-keys": false
    },
    
    "rulesDirectory": []
}